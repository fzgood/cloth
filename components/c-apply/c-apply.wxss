.c-apply{
  position: fixed;
  top:0;
  left:0;
  width: 100vw;
  height: 100vh;
  z-index: 999;
}
.c-apply__mask{
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height: 100%;
  background-color:rgba(0, 0, 0, 0.6);
}
.c-apply__box{
  position: absolute;
  bottom:0;
  left:0%;
  width: 100%;
  height: 60%;
  background-color:#fff;
  border-radius:10px 10px 0 0 ;
}