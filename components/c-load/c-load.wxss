.c-load{
  position: fixed;
  top:50%;
  left:50%;
  transform: translate(-50%, -50%);
}
.loader {
  position: relative;
  width: 188rpx;
  height: 200rpx;
  border-radius: 50%;
  display: inline-block;
  vertical-align: middle;
}

.loading11 .line {
  width: 8rpx;
  position: absolute;
  border-radius: 5rpx;
  bottom: 0;
  background: -webkit-gradient(
    linear,
    left top,
    left bottom,
    from(#1ee95d),
    to(#5714ce)
  );
  background: -webkit-linear-gradient(top, #1ee95d, #5714ce);
  background: linear-gradient(to bottom, #1ee95d, #5714ce);
}
.loading11 .line1 {
  left: 0;
  animation: line-grow 1.5s ease alternate infinite;
}
.loading11 .line2 {
  left: 20rpx;
  animation: line-grow 1.5s 0.2s ease alternate infinite;
}
.loading11 .line3 {
  left: 40rpx;
  animation: line-grow 1.5s 0.4s ease alternate infinite;
}
.loading11 .line4 {
  left: 60rpx;
  animation: line-grow 1.5s 0.6s ease alternate infinite;
}
.loading11 .line5 {
  left: 80rpx;
  animation: line-grow 1.5s 0.8s ease alternate infinite;
}
.loading11 .line6 {
  left: 100rpx;
  animation: line-grow 1.5s 1.0s ease alternate infinite;
}
.loading11 .line7 {
  left: 120rpx;
  animation: line-grow 1.5s 1.2s ease alternate infinite;
}
.loading11 .line8 {
  left: 140rpx;
  animation: line-grow 1.5s 1.4s ease alternate infinite;
}
.loading11 .line9 {
  left: 160rpx;
  animation: line-grow 1.5s 1.6s ease alternate infinite;
}
.loading11 .line10 {
  left: 180rpx;
  animation: line-grow 1.5s 1.8s ease alternate infinite;
}
@-webkit-keyframes line-grow {
  0% {
    height: 0;
  }
  100% {
    height: 75%;
  }
}
@keyframes line-grow {
  0% {
    height: 0;
  }
  100% {
    height: 100%;
  }
}